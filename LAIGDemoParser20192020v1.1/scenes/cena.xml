<lxs>
    <scene root="myRoot" axis_length="10" />

    <views default="defaultcamera" >
        <perspective id="defaultCamera" near="0.1" far="500" angle="45">
            <from x="30" y="15" z="30" />
            <to x="0" y="-2" z="0" />
        </perspective>
    </views>

    <globals>  
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <background r="0.0" g="0.0" b="0.0" a="1.0" />
    </globals>

    <lights>
        <omni id="demoLight" enabled="1">
            <location x="9.8" y="11.9" z="10" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </omni>
    </lights>

    <textures>
        <texture id="bankTexture" file="scenes/images/bank.jpg" />  
    </textures>

    <materials> 
        <material id="demoMaterial" shininess="10">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.0" g="0.0" b="0.3" a="1.0" />
            <diffuse r="0.6" g="0.0" b="0.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </material>
        <material id="vidralMaterial" shininess = "100" >
            <emission r="0.5" g="0.5" b="0.5" a="1.0" />
            <ambient r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="0.0" g="0.0" b="0.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </material> 
    </materials>

    <transformations>
        <transformation id="scaleBubble">
            <scale x="0.3" y="0.3" z="0.3" />
        </transformation>
    </transformations>

    <primitives>
        <primitive id="myRectangle">
            <rectangle x1="-0.5" y1="-1" x2="0.5" y2="1" />
        </primitive>
        <primitive id="myTriangle1">
            <triangle x1="0" y1="0" z1="-3" x2="0" y2="2" z2="0" x3="0" y3="0" z3="0"/>
        </primitive>
        <primitive id="myTriangle2">
            <triangle x1="0" y1="2" z1="0" x2="0" y2="0" z2="3" x3="0" y3="0" z3="0"/>
        </primitive>
        <primitive id="myCylinder">
            <cylinder base="0.1" top="0.1" height="2" slices="20" stacks="4" />
        </primitive>
        <primitive id="myTail">
            <cylinder base="0.1" top="0.3" height="0.3" slices="6" stacks="4" />
        </primitive>
        <primitive id="mySphere">
            <sphere radius="1" slices="50" stacks="50" />
        </primitive>
        <primitive id="myTorus">
            <torus inner="0.5" outer="1" slices="50" loops="50" />
        </primitive>
    </primitives >
 
    <components>    
        <component id="stick">
            <transformation>
                 <rotate axis="x" angle="-90" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <primitiveref id="myCylinder" />
            </children>
        </component>

        <component id="vela">
            <transformation>
                <translate x="0" y="1.2" z="0" />
                <scale x="0.3" y="0.3" z="0.3" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <primitiveref id="myTriangle1" />
            </children>
        </component>  

        <component id="mastro">
            <transformation>
                <translate x="-1" y="0" z="-1" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <componentref id="stick" />
                <componentref id="vela" />
            </children>
        </component>

        <component id="middle1">
            <transformation>
                <scale x="3" y="1" z="1" />
                <rotate axis="x" angle="-90" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <primitiveref id="myRectangle" />
            </children>
        </component>

        <component id="middle2">
            <transformation>
                <scale x="3" y="1" z="1" />
                <rotate axis="x" angle="90" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <primitiveref id="myRectangle" />
            </children>
        </component> 

        <component id="middle3">
            <transformation>
                <scale x="3" y="1" z="1" />
                <rotate axis="x" angle="-90" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <primitiveref id="myTorus" />
            </children>
        </component> 

        <component id="boat">
            <transformation>
                <translate x="0" y="0" z="-10" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <componentref id="mastro" />
                <componentref id="middle1" />
                <componentref id="middle2" />
                <componentref id="middle3" />
            </children>
        </component> 

        <component id="bubble1">
            <transformation>
                <scale x="0.3" y="0.3" z="0.3" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <primitiveref id="mySphere" />
            </children>
        </component> 

        <component id="bubble2">
            <transformation>
                <translate x="0.3" y="-0.4" z="0" />
                <scale x="0.2" y="0.2" z="0.2" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <primitiveref id="mySphere" />
            </children>
        </component>

        <component id="bubble3">
            <transformation>
                <translate x="-0.1" y="-0.9" z="0" />
                <scale x="0.2" y="0.2" z="0.2" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <primitiveref id="mySphere" />
            </children>
        </component>

        <component id="bubbles1">
            <transformation>
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <componentref id="bubble1" />
                <componentref id="bubble2" />
                <componentref id="bubble3" />
            </children>
        </component>

        <component id="bubbles2">
            <transformation>
                <translate x="0.5" y="-1.5" z="0" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <componentref id="bubble1" />
                <componentref id="bubble2" />
                <componentref id="bubble3" />
            </children>
        </component>

        <component id="bubbles3">
            <transformation>
                <translate x="0" y="-3" z="0" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <componentref id="bubble1" />
                <componentref id="bubble2" />
                <componentref id="bubble3" />
            </children>
        </component>

        <component id="bubbleChain">
            <transformation>
                <translate x="0" y="-3" z="0" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <componentref id="bubbles1" />
                <componentref id="bubbles2" />
                <componentref id="bubbles3" />
            </children>
        </component>

        <component id="body">
            <transformation>
                <scale x="0.6" y="0.3" z="0.4" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <primitiveref id="mySphere" />
            </children>
        </component>

        <component id="stripe1">
            <transformation>
                <scale x="0.2" y="0.22" z="0.3" />
                <rotate axis="y" angle="-90" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <primitiveref id="myTorus" />
            </children>
        </component>

        <component id="stripe2">
            <transformation>
                <translate x="0.2" y="0" z="0" />
                <scale x="0.12" y="0.2" z="0.28" />
                <rotate axis="y" angle="-90" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <primitiveref id="myTorus" />
            </children>
        </component>

        <component id="fin1">
            <transformation>
                <scale x="0.2" y="0.2" z="0.1" />
                <rotate axis="y" angle="90" />
                <translate x="0" y="1" z="1" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <primitiveref id="myTriangle1" />
            </children>
        </component>

        <component id="fin2">
            <transformation>
                <scale x="0.2" y="0.2" z="0.1" />
                <rotate axis="y" angle="-90" />
                <translate x="0" y="1" z="-1" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <primitiveref id="myTriangle2" />
            </children>
        </component>

        <component id="eye1">
            <transformation>
                <translate x="-0.3" y="0" z="0.3" />
                <scale x="0.1" y="0.1" z="0.1" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <primitiveref id="mySphere" />
            </children>
        </component>

        <component id="eye2">
            <transformation>
                <translate x="-0.3" y="0" z="-0.3" />
                <scale x="0.1" y="0.1" z="0.1" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <primitiveref id="mySphere" />
            </children>
        </component>

        <component id="tail">
            <transformation>
                <translate x="-0.3" y="2" z="-0.3" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <primitiveref id="myTail" />
            </children>
        </component>

        <component id="fish">
            <transformation>
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <componentref id="body" />
                <componentref id="stripe1" />
                <componentref id="stripe2" />
                <componentref id="eye1" />
                <componentref id="eye2" />
                <componentref id="fin1" />
                <componentref id="fin2" />
                <componentref id="tail" />
            </children>
        </component>

        <component id="myRoot">
            <transformation>
                 <scale x="2" y="2" z="2" />
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="bankTexture"/>
            <children>
                <componentref id="bubbleChain" />
                <componentref id="boat" />
                <componentref id="fish" />
            </children>
        </component> 
    </components>
   
</lxs>